<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>
<ul>
  {{#each companies}}
    <li>
      <a href="/detail/{{this._id}}/home">
        {{ this.name }}
        {{ this.lastPrice}}
      </a>
      <form action="/list/{{this._id}}/add" method="POST">
        <button>Add</button>
      </form>
    </li>
  {{/each}}
</ul>